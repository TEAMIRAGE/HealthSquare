<%- include('header.ejs') %>

<!-- component -->
<div class="bg-gray-900 min-h-screen flex items-center justify-center pb-24">
  <div
    class="mt-44 bg-gray-800 flex-1 flex flex-col max-w-6xl sm:p-6 sm:mx-4 sm:rounded-2xl"
  >
    <!-- Content -->
    <div class="flex-1 px-2 sm:px-0">
      <div class="flex justify-between items-center">
        <h3 class="text-3xl font-extralight text-white">Diagnosed Reports</h3>
        <div class="inline-flex items-center space-x-2">
          <a href="/mediLocker">
            <button
              class="border-2 border-white border-solid px-8 py-2 font-bold text-white bg-transparent rounded-xl duration-500 hover:scale-105"
            >
              Access HealthLocker
            </button>
          </a>
        </div>
      </div>
      <div
        class="mb-10 sm:mb-0 mt-10 grid gap-4 grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4"
      >
        <% bloodReportSignedURLs.forEach((info) => { %>
        <div
          class="relative group bg-gray-900 py-10 sm:py-20 px-4 flex flex-col space-y-2 items-center cursor-pointer rounded-md hover:bg-gray-900/80 hover:smooth-hover"
        >
          <a href="<%= info.signedURL %>">
            <img
              class="w-20 h-20 object-cover object-center rounded-full"
              src="/images/pdf-image.png"
              alt=""
            />
          </a>
          <h4 class="text-white text-2xl font-bold capitalize text-center">
            <a href="<%= info.signedURL %>"> Result </a>
          </h4>
          <h4 class="text-white capitalize text-center"><%= info.date %></h4>

          <a
            href="/crud/delete-bloodReport-collection/<%= info.uniqueID %>"
            class="px-8 py-2 rounded-xl cursor-pointer border-2 border-solid border-white bg-transparent text-white/50 duration-500 hover:scale-105"
            >Delete</a
          >
          <p class="absolute top-2 text-white inline-flex items-center text-sm">
            Blood Report
          </p>
        </div>
        <% }); %> <% tumorSignedURLs.forEach((info) => { %>
        <div
          class="relative group bg-gray-900 py-10 sm:py-20 px-4 flex flex-col space-y-2 items-center cursor-pointer rounded-md hover:bg-gray-900/80 hover:smooth-hover"
        >
          <a href="<%= info.signedURL %>">
            <img
              class="w-20 h-20 object-cover object-center rounded-full"
              src="<%= info.signedURL %>"
              alt=""
            />
          </a>
          <h4 class="text-white text-2xl font-bold capitalize text-center">
            <%= info.output %>
          </h4>
          <h4 class="text-white capitalize text-center"><%= info.date %></h4>
          <a
            href="/crud/delete-tumor-collection/<%= info.uniqueID %>"
            class="px-8 py-2 rounded-xl cursor-pointer border-2 border-solid border-white bg-transparent text-white/50 duration-500 hover:scale-105"
            >Delete</a
          >
          <p class="absolute top-2 text-white inline-flex items-center text-sm">
            Tumor Report
          </p>
        </div>
        <% }); %> <% fractureSignedURLs.forEach((info) => { %>
        <div
          class="relative group bg-gray-900 py-10 sm:py-20 px-4 flex flex-col space-y-2 items-center cursor-pointer rounded-md hover:bg-gray-900/80 hover:smooth-hover"
        >
          <a href="<%= info.signedURL %>">
            <img
              class="w-20 h-20 object-cover object-center"
              src="<%= info.signedURL %>"
              alt=""
            />
          </a>
          <h4 class="text-white text-2xl font-bold capitalize text-center">
            <%= info.output %>
          </h4>
          <h4 class="text-white capitalize text-center"><%= info.date %></h4>
          <a
            href="/crud/delete-fracture-collection/<%= info.uniqueID %>"
            class="px-8 py-2 rounded-xl cursor-pointer border-2 border-solid border-white bg-transparent text-white/50 duration-500 hover:scale-105"
            >Delete</a
          >
          <p class="absolute top-2 text-white inline-flex items-center text-sm">
            Fracture Report
          </p>
        </div>
        <% }); %>
      </div>
    </div>
  </div>
</div>

